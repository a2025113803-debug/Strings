#include <iostream>
#include <string>
using namespace std;

int main() {
    string texto;
    getline(cin, texto);

    int palabras = 0;
    bool enPalabra = false;

    for (char c : texto) {
        if (isspace(c)) enPalabra = false;
        else if (!enPalabra) {
            enPalabra = true;
            palabras++;
        }
    }
    cout << palabras << " palabras";
}
